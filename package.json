{
    "name": "svelte-app",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite dev --host 0.0.0.0",
        "build": "svelte-kit sync && vite build",
        "preview": "vite preview",
        "prepare": "svelte-kit sync",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint:es": "eslint --ext .js,.ts,.svelte --ignore-path .gitignore .",
        "lint:es:fix": "eslint --fix --ext .js,.ts,.svelte --ignore-path .gitignore .",
        "lint:style": "stylelint ./src/**/*.{css,scss,svelte} --ignore-pattern .gitignore",
        "lint:style:fix": "stylelint --fix ./src/**/*.{css,scss,svelte} --ignore-pattern .gitignore"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.3.1",
        "@sveltejs/adapter-cloudflare": "^4.7.4",
        "@sveltejs/adapter-node": "^5.2.9",
        "@sveltejs/kit": "^2.8.4",
        "@sveltejs/vite-plugin-svelte": "^5.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "carbon-components-svelte": "^0.85.0",
        "eslint": "^9.0.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^17.0.0",
        "eslint-plugin-promise": "^7.0.0",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-plugin-svelte": "^3.0.0",
        "postcss-html": "^1.5.0",
        "prettier-plugin-svelte": "^3.0.0",
        "stylelint": "^16.0.0",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-recess-order": "^7.0.0",
        "stylelint-config-standard": "^39.0.0",
        "svelte": "^5.2.8",
        "svelte-check": "^4.1.0",
        "svelte-preprocess": "^6.0.3",
        "tslib": "^2.5.0",
        "typescript": "^5.0.0",
        "vite": "^6.0.0",
        "wrangler": "^3.52.0"
    },
    "dependencies": {}
}
